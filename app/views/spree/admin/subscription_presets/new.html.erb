<% admin_breadcrumb(t('.title')) %>

<% content_for :page_actions do %>
  <li><%= button_link_to t('.back'), admin_subscription_presets_path %></li>
<% end %>

<%= render partial: 'spree/shared/error_messages', locals: { target: @subscription_preset } %>

<%= form_for [:admin, @subscription_preset] do |f| %>
  <fieldset data-hook="new_subscription_preset" class="no-border-top">
    <%= render partial: 'form', locals: { f: f } %>
    <div class="clear"></div>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>

